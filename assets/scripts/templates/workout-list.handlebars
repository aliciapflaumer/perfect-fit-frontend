{{#each workouts as |workouts|}}

<ul id="workout-listing" data-id="{{workouts.id}}">
  <li>Exercise ID: {{workouts.id}}</li>
  <li>Name: {{workouts.name}}</li>
  <li>Duration: {{workouts.duration}}</li>
  <li>Repetition: {{workouts.repetitions}}</li>
  <!-- workout[plan_id] -->
  <li>Plan Id: {{workouts.plan_id}}</li>
</ul>
{{/each}}
